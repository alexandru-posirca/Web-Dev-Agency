---
interface Props {
  content: string;
}

const { content } = Astro.props;
---

<div class="content-bottom-sec">
  <hr class="opacity-40" />
  <div class="text-content-sec my-6 max-w-[700px] md:my-8 text-center">
    <p>
      {content}
    </p>
    <slot />
  </div>
  <hr class="opacity-40" />
</div>
